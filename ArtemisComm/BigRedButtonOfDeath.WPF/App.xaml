<Application x:Class="BigRedButtonOfDeath.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml" DispatcherUnhandledException="OnUnhandledException">
    <Application.Resources>
        <ImageSource x:Key="plusSign">pack://application:,,,/Resources/plus.png</ImageSource>
        <ImageSource x:Key="CancelSelfDestruct">pack://application:,,,/Resources/cancelselfdestruct.png</ImageSource>
        <ImageSource x:Key="SelfDestruct">pack://application:,,,/Resources/selfdestruct.png</ImageSource>
    </Application.Resources>
</Application>
